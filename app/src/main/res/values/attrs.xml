<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="ViewFlipperWithLayoutEditorPreview">
    <attr name="debug_displayedChild" format="integer" />
  </declare-styleable>

  <declare-styleable name="ToolbarWithTintedNavIcon">
    <attr name="navigationIconTint" format="color" />
  </declare-styleable>

  <declare-styleable name="PrimarySolidButtonWithFrame">
    <attr name="frameBackgroundEnabled" format="color" />
    <attr name="frameBackgroundDisabled" format="color" />
    <attr name="buttonBackgroundEnabled" format="color" />
    <attr name="buttonBackgroundDisabled" format="color" />
  </declare-styleable>

  <declare-styleable name="MedicalHistoryQuestionView">
    <attr name="contentPaddingStart" format="dimension" />
    <attr name="contentPaddingEnd" format="dimension" />
  </declare-styleable>

  <declare-styleable name="PinEntryCardView">
    <attr name="verificationMethod" format="enum">
      <enum name="local" value="0" />
      <enum name="ongoingEntry" value="1" />
      <enum name="serverLogin" value="2" />
    </attr>
  </declare-styleable>

  <declare-styleable name="ScreenNavigator">
    <attr name="layout" format="reference" />
    <attr name="analyticsName" format="string" />
  </declare-styleable>

  <declare-styleable name="ProgressMaterialButton">
    <attr name="buttonState" format="enum">
      <enum name="in-progress" value="0" />
      <enum name="enabled" value="1" />
      <enum name="disabled" value="2" />
    </attr>
  </declare-styleable>

  <declare-styleable name="FacilityPickerView">
    <attr name="pickFrom" format="enum">
      <enum name="all-facilities" value="0" />
      <enum name="in-current-group" value="1" />
    </attr>
  </declare-styleable>

  <declare-styleable name="PatientStatusView">
    <attr name="statusIcon" format="reference" />
    <attr name="statusText" format="string" />
    <attr name="statusTextColor" format="color" />
    <attr name="statusIconTint" format="color" />
    <attr name="statusTextAppearance" format="reference" />
  </declare-styleable>

  <declare-styleable name="ChipInputAutoCompleteTextView">
    <attr name="hint" format="string" />
  </declare-styleable>
</resources>
